<%= simple_form_for(@item) do |f| %>
  <%= f.input :category %>
  <%= f.input :name %>
  <%= f.input :brand %>
  <%= f.input :size %>
  <%= f.input :color %>
  <%= f.input :photo, as: :file%>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :availability%>
  <%= f.submit %>
<% end %>
